          <script type="text/javascript" src="/portal_attendance_pro/static/src/js/geofence_model.js"></script>
          <script type="text/javascript" src="/portal_attendance_pro/static/src/js/geofence_controller.js"></script>
          <script type="text/javascript" src="/portal_attendance_pro/static/src/js/geofence_renderer.js"></script>
          <script type="text/javascript" src="/portal_attendance_pro/static/src/js/geofence_view.js"></script>

          <script type="text/javascript" src="/portal_attendance_pro/static/src/js/geofence_common.js"></script>
        </xpath>
    </template>

</odoo>